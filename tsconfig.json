{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES6",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6"
  }
}